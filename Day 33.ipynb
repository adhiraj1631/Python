   "source": [
    "def factorial(n):\n",
    "    fact=1\n",
    "    for i in range(1,n+1):\n",
    "        fact=fact*i\n",
    "    return fact\n",
    "f=factorial(int(input(\"Enter the number:\")))\n",
    "print(f)"
