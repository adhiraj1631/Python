   ],
   "source": [
    "def factorial(n):\n",
    "    f=1\n",
    "    for i in range (1,n+1):\n",
    "        f=f*i\n",
    "    return f\n",
    "fact=factorial(5)\n",
    "print(fact)"
   ]
